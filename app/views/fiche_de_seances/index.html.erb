<p id="notice"><%= notice %></p>

<h1>Fiche De Seances</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Cohorte</th>
      <th>Theme</th>
      <th>Titre</th>
      <th>Formateur</th>
      <th>Effectif</th>
      <th>Lieux</th>
      <th>Duree</th>
      <th>Seance precedente</th>
      <th>Seance suivante</th>
      <th>Reference</th>
      <th>Moyen pedagogique</th>
      <th>Objectif</th>
      <th>Plan</th>
      <th>Points cles</th>
      <th>Introduction</th>
      <th>Premiere part</th>
      <th>Deux part</th>
      <th>Trois part</th>
      <th>Conclusion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fiche_de_seances.each do |fiche_de_seance| %>
      <tr>
        <td><%= fiche_de_seance.date %></td>
        <td><%= fiche_de_seance.cohorte %></td>
        <td><%= fiche_de_seance.theme %></td>
        <td><%= fiche_de_seance.titre %></td>
        <td><%= fiche_de_seance.formateur %></td>
        <td><%= fiche_de_seance.effectif %></td>
        <td><%= fiche_de_seance.lieux %></td>
        <td><%= fiche_de_seance.duree %></td>
        <td><%= fiche_de_seance.seance_precedente %></td>
        <td><%= fiche_de_seance.seance_suivante %></td>
        <td><%= fiche_de_seance.reference %></td>
        <td><%= fiche_de_seance.moyen_pedagogique %></td>
        <td><%= fiche_de_seance.objectif %></td>
        <td><%= fiche_de_seance.plan %></td>
        <td><%= fiche_de_seance.points_cles %></td>
        <td><%= fiche_de_seance.introduction %></td>
        <td><%= fiche_de_seance.premiere_part %></td>
        <td><%= fiche_de_seance.deux_part %></td>
        <td><%= fiche_de_seance.trois_part %></td>
        <td><%= fiche_de_seance.conclusion %></td>
        <td><%= link_to 'Show', fiche_de_seance %></td>
        <td><%= link_to 'Edit', edit_fiche_de_seance_path(fiche_de_seance) %></td>
        <td><%= link_to 'Destroy', fiche_de_seance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

